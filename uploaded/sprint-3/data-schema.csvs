version 1.0
@separator ','
@totalColumns 23
@ignoreColumnNameCase
eaID: notEmpty if(length(1,30), notEmpty regex("^[0-9a-zA-Z]{1,30}$"))
siteName: length(0,255) regex("^[0-9a-zA-Z," "]{0,255}$")
retType: is("Return type 1") or is("Return type 2") or is("1234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456") length(*,255) //Value from Controlled List
dateSt: 
dateEn: if(not(""),ukDate)
periodName: 
periodYear: if($dateSt/is(""),notEmpty range(1970,2999))
sampleRef: length(0,255) regex("^[0-9a-zA-Z," "]{0,255}$")
sampledBy: is("Sampled by 1") or is("Sampled by 2") or is("123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345678901234567890123456789012345") length(*,255)length(1,255) regex("^[0-9a-zA-Z," "]{1,255}$")
monPoint: notEmpty if(length(1,255), regex("^[0-9a-zA-Z," "]{1,255}$"))
parameter: is("Temperature") or is("Humidity") length(*,255) //Value from Controlled List
value: length(*,30) //Parsed to number if value is numeric
tValue:
unit: is("Metres") or is("Mbar") length(*,30) //Value from Controlled List
measurePeriod: is("Annual") or is("Quarter 1")length(*,250) //Value from Controlled List
testMethod: is("Test method 1") or is("Test method 2") length(*,30) //Value from standard list.  Return specific
comments: length(*,255)
CiC: is("Yes") or is("No") or is("True") or is("False") or is("")
addParameter1: length(*,255)
addparameter2: length(*,255)
addparameter3: length(*,255)
addparameter4: length(*,255)
addparameter5: length(*,255)
